####################################################
# 1、定义编译器及环境公共参数，方便不同环境/平台移植
####################################################

CC = gcc
AR = ar
RM = rm
CFLAG_L = 
CFLAG_O = -c -W -Wall
CFLAG_M = -E -MM
ARFLAGS = rcs
RMFLAGS = -fr
MKDIR   = mkdir -p
ECHO_SET= @set -e
ECHO_END= 
ifdef VPATH
SRCS_INC := $(addprefix -I,$(strip $(VPATH)))
endif
